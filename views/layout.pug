  script(src="/assets/js/plugins/jquery.dataTables.min.js")
  // Full Calendar
  script(src="/assets/js/plugins/fullcalendar.min.js")
  script(src="/assets/js/light-bootstrap-dashboard.js?v=2.0.1" type="text/javascript")
  script(src="/assets/js/demo.js")
